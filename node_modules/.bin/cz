#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aragi/Documents/server-minecraft/node_modules/.pnpm/commitizen@4.3.0_@types+node@22.0.2_typescript@5.5.4/node_modules/commitizen/bin/node_modules:/home/aragi/Documents/server-minecraft/node_modules/.pnpm/commitizen@4.3.0_@types+node@22.0.2_typescript@5.5.4/node_modules/commitizen/node_modules:/home/aragi/Documents/server-minecraft/node_modules/.pnpm/commitizen@4.3.0_@types+node@22.0.2_typescript@5.5.4/node_modules:/home/aragi/Documents/server-minecraft/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/aragi/Documents/server-minecraft/node_modules/.pnpm/commitizen@4.3.0_@types+node@22.0.2_typescript@5.5.4/node_modules/commitizen/bin/node_modules:/home/aragi/Documents/server-minecraft/node_modules/.pnpm/commitizen@4.3.0_@types+node@22.0.2_typescript@5.5.4/node_modules/commitizen/node_modules:/home/aragi/Documents/server-minecraft/node_modules/.pnpm/commitizen@4.3.0_@types+node@22.0.2_typescript@5.5.4/node_modules:/home/aragi/Documents/server-minecraft/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../commitizen/bin/git-cz" "$@"
else
  exec node  "$basedir/../commitizen/bin/git-cz" "$@"
fi
